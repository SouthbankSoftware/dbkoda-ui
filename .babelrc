{
  "presets": [
    "env",
    "react",
    "flow"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-rest-spread",
    "add-module-exports", 
    ["babel-plugin-root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "src"
    }, {
      "rootPathPrefix": "#",
      "rootPathSuffix": "src/components"
    }]],
    "react-hot-loader/babel"
  ]
}
