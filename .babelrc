{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-async-to-generator",
    "transform-object-rest-spread", ["babel-plugin-root-import", [{
      "rootPathPrefix": "~",
      "rootPathSuffix": "src"
    }, {
      "rootPathPrefix": "#",
      "rootPathSuffix": "src/components"
    }]],
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-async-to-generator",
        "transform-object-rest-spread", ["babel-plugin-root-import", [{
          "rootPathPrefix": "~",
          "rootPathSuffix": "src"
        }, {
          "rootPathPrefix": "#",
          "rootPathSuffix": "src/components"
        }]],
        "react-hot-loader/babel"
      ]
    }
  }
}
