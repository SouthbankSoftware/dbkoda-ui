{
  "Title": "Export Database",
  "DefaultValues": {
    "function": "dbkoda_ExportDatabasePreFill",
    "arguments": [
      {
        "name": "Database",
        "value": "treeNode"
      }
    ]
  },
  "Fields": [
    {
      "name": "Database",
      "type": "Text",
      "readOnly": true
    },{
      "name": "allCollections",
      "type": "Boolean",
      "readOnly": false,
      "tooltip": "Backup all collections?",
      "rules": "boolean"
    },
    {
      "name": "Collections",
      "type": "Table",
      "columns": [
        {
          "name": "CollectionName",
          "label": "Collection Name",
          "tooltip": "Name of the collection to backup",
          "type": "Combo",
          "lookup": "dbkoda_listcollections",
          "lookup_arguments": [
            {
              "name": "db",
              "value": "treeNode"
            },
            {
              "name": "collection",
              "value": "treeNode"
            }
          ]
        }
      ]
    }
  ]
}