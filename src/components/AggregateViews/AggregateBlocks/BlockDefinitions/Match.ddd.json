{
  "Title": "Match",
  "DefaultValues": {},
  "Fields": [
    {
      "name": "filter",
      "label": "Filter Operator",
      "tooltip": "Apply filter operator across filter conditions",
      "type": "Radio",
      "defaultValue": "none",
      "radios": [
        {
          "label": "none",
          "value": "none"
        },
        {
          "label": "Use Or?",
          "value": "UseOr"
        },
        {
          "label": "Use And?",
          "value": "UseAnd"
        }
      ]
    },
    {
      "name": "FilterKeys",
      "label": "Filter conditions",
      "type": "Table",
      "columns": [
        {
          "name": "AttributeName",
          "label": "Attribute",
          "tooltip": "Name of the attribute to be filtered",
          "type": "Combo",
          "lookup": "dbkodaListAttributesAgg",
          "lookup_arguments": [
            {
              "name": "prevAttributes",
              "value": "prevAttributes"
            }
          ]
        },
        {
          "name": "Operator",
          "label": "Operator",
          "tooltip": "Operator to apply to value",
          "type": "Combo",
          "lookup": "dbkodaMatchOperators",
          "lookup_arguments": [
            {
              "name": "db",
              "value": "database"
            }
          ]
        },
        {
          "name": "Value",
          "label": "Value to filter",
          "type": "Text",
          "tooltip": "Enter the value to match on(quote strings please)"
        }
      ]
    }
  ]
}
