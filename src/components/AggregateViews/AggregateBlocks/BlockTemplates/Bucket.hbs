{
  $bucket: {
           groupBy: "${{{groupBy}}}", 
      {{#if defaultValue}} 
           default: {{{defaultValue}}} , 
      {{/if ~}}
      {{#if AggregateKeys }}  
           output: { "count":{$sum:1}
        {{#each AggregateKeys}}
                    ,"{{nodots AttributeName}}-{{{Aggregation}}}":{${{{Aggregation}}}:"${{{AttributeName}}}"} {{/each}} 
            }, 
      {{/if }}
            boundaries: [ {{#each lowerBounds}} {{{lowerBound}}}{{#unless last}},{{/unless}}{{/each}} ]
   }
}