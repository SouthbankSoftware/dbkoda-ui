{
  "name": "@southbanksoftware/dbenvy-ui",
  "version": "0.1.0",
  "description": "DBEnvy UI",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack --config config/webpack.config.js && webpack-dev-server --config config/webpack.config.js --hot --history-api-fallback --port 3000 --content-base dist",
    "build": "yarn run clean && rimraf ./dist && webpack --config config/webpack.config.prod.js",
    "preview": "http-server ./dist -p 1234 -c-1",
    "test": "npm run eslint && nyc npm run mocha",
    "eslint": "eslint src/. tests/.",
    "mocha": "NODE_ENV=test babel-node ./node_modules/mocha/bin/_mocha --opts mocha.opts ./src/**/*.test.js*",
    "jest": "jest --config jest.json --coverage",
    "test-target": "NODE_ENV=test mocha --opts mocha.opts",
    "docs": "esdoc -c .esdoc.json && open ./docs/index.html",
    "clean": "rimraf ./dist rmdir",
    "drone:sign": "drone sign \"SouthbankSoftware/$npm_package_name\""
  },
  "keywords": [
    "React",
    "React-Router",
    "MobX",
    "Webpack",
    "ES6",
    "ES7"
  ],
