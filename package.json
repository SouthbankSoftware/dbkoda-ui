{
  "name": "@southbanksoftware/dbenvy-ui",
  "gitName": "dbenvy-ui",
  "fullName": "DBEnvy UI",
  "description": "DBEnvy UI built with state of the art React stack",
  "version": "0.1.1",
  "author": {
    "name": "Southbank Software",
    "email": "developer@southbanksoftware.com",
    "url": "http://southbanksoftware.com"
  },
  "main": "dist/index.html",
  "keywords": [
    "React",
    "React-Router",
    "MobX",
    "Webpack",
    "ES6",
    "ES7"
  ],
  "engines": {
    "node": ">= 7.2.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.config.js",
    "build": "yarn run clean && rimraf ./dist && webpack --config config/webpack.config.js",
    "preview": "http-server ./dist -p 1234 -c-1",
    "test": "yarn run jest && yarn run eslint",
    "eslint": "eslint --ext .jsx --ext .js src/. tests/.",
    "jest": "NODE_ENV=test jest --config jest.json --coverage",
    "docs": "yarn run esdoc && open -b com.google.chrome ./docs/index.html",
    "esdoc": "rimraf docs && esdoc -c .esdoc.json",
    "clean": "rimraf ./dist rmdir",
    "cienvy:dev": "yarn run cienvy:clean && yarn run cienvy:dev:build",
    "cienvy:dev:build": "yarn run cienvy:build -- --source-maps",
    "cienvy:clean": "rimraf .cienvy/lib",
    "cienvy:build": "BABEL_ENV=cienvy babel -d .cienvy/lib .cienvy/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SouthbankSoftware/dbenvy-ui.git"
  },
  "license": "BSD",
  "private": true,
  "bugs": {
    "url": "https://github.com/SouthbankSoftware/dbenvy-ui/issues"
  },
  "homepage": "https://github.com/SouthbankSoftware/dbenvy-ui#readme",
